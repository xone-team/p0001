<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xone.model.hibernate.entity.Category" table="T_CATEGORY">
	<id name="id" type="java.lang.Long">
		<column name="ID" length="20" not-null="true"/>
		<generator class="increment" />
	</id>
	<property name="name" type="java.lang.String">
		<column name="NAME" length="1023"/>
	</property>
	<property name="detail" type="java.lang.String">
		<column name="DETAIL" length="1023"/>
	</property>
	<property name="sort" type="java.lang.Integer">
		<column name="SORT" length="11"/>
	</property>
	<property name="parentId" type="java.lang.Long">
		<column name="PARENT_ID" length="20"/>
	</property>
	<property name="flagDeleted" type="java.lang.String">
		<column name="FLAG_DELETED" length="1"/>
	</property>
	<property name="userCreated" type="java.lang.Long">
		<column name="USER_CREATED" length="20"/>
	</property>
	<property name="dateCreated" type="java.util.Date">
		<column name="DATE_CREATED" length="19"/>
	</property>
	<property name="userUpdated" type="java.lang.Long">
		<column name="USER_UPDATED" length="20"/>
	</property>
	<property name="lastUpdated" type="java.util.Date">
		<column name="LAST_UPDATED" length="19"/>
	</property>
	</class>
</hibernate-mapping>
