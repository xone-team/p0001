<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xone.model.hibernate.entity.CompanyInfo" table="T_COMPANY_INFO">
	<id name="id" type="java.lang.Long">
		<column name="ID" length="20" not-null="true"/>
		<generator class="increment" />
	</id>
	<property name="name" type="java.lang.String">
		<column name="NAME" length="255"/>
	</property>
	<property name="address" type="java.lang.String">
		<column name="ADDRESS" length="255"/>
	</property>
	<property name="tel" type="java.lang.String">
		<column name="TEL" length="25"/>
	</property>
	<property name="mobile" type="java.lang.String">
		<column name="MOBILE" length="15"/>
	</property>
	<property name="contact" type="java.lang.String">
		<column name="CONTACT" length="15"/>
	</property>
	<property name="fax" type="java.lang.String">
		<column name="FAX" length="25"/>
	</property>
	<property name="intro" type="java.lang.String">
		<column name="INTRO" length="255"/>
	</property>
	<property name="userId" type="java.lang.Long">
		<column name="USER_ID" length="20"/>
	</property>
	<property name="userCreated" type="java.lang.Long">
		<column name="USER_CREATED" length="20"/>
	</property>
	<property name="dateCreated" type="java.util.Date">
		<column name="DATE_CREATED" length="19"/>
	</property>
	<property name="userUpdated" type="java.lang.Long">
		<column name="USER_UPDATED" length="20"/>
	</property>
	<property name="lastUpdated" type="java.util.Date">
		<column name="LAST_UPDATED" length="19"/>
	</property>
	</class>
</hibernate-mapping>
