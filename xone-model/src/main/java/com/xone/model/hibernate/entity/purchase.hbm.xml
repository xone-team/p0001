<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.xone.model.hibernate.entity.Purchase" table="T_PURCHASE">
        <id name="id" type="java.lang.Long">
            <column name="ID" length="20" not-null="true" />
            <generator class="increment" />
        </id>
        <property name="purchaseName" type="java.lang.String">
            <column name="PURCHASE_NAME" length="255" />
        </property>
        <property name="purchaseType" type="java.lang.String">
            <column name="PURCHASE_TYPE" length="2" />
        </property>
        <property name="checkStatus" type="java.lang.String">
            <column name="CHECK_STATUS" length="1" />
        </property>
        <property name="purchaseNum" type="java.lang.String">
            <column name="PURCHASE_NUM" length="255" />
        </property>
        <property name="purchaseValid" type="java.util.Date">
            <column name="PURCHASE_VALID" length="19" />
        </property>
        <property name="purchaseAddress" type="java.lang.String">
            <column name="PURCHASE_ADDRESS" length="255" />
        </property>
        <property name="purchaseLocation" type="java.lang.String">
            <column name="PURCHASE_LOCATION" length="255" />
        </property>
        <property name="purchaseDesc" type="java.lang.String">
            <column name="PURCHASE_DESC" length="255" />
        </property>
        <property name="flagDeleted" type="java.lang.String">
            <column name="FLAG_DELETED" length="1" />
        </property>
        <property name="userApply" type="java.lang.Long">
            <column name="USER_APPLY" length="20" />
        </property>
        <property name="dateApply" type="java.util.Date">
            <column name="DATE_APPLY" length="19" />
        </property>
        <property name="userCheck" type="java.lang.Long">
            <column name="USER_CHECK" length="20" />
        </property>
        <property name="dateCheck" type="java.util.Date">
            <column name="DATE_CHECK" length="19" />
        </property>
        <property name="userCreated" type="java.lang.Long">
            <column name="USER_CREATED" length="20" />
        </property>
        <property name="dateCreated" type="java.util.Date">
            <column name="DATE_CREATED" length="19" />
        </property>
        <property name="userUpdated" type="java.lang.Long">
            <column name="USER_UPDATED" length="20" />
        </property>
        <property name="lastUpdated" type="java.util.Date">
            <column name="LAST_UPDATED" length="19" />
        </property>
    </class>
</hibernate-mapping>