<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>  
    <class name="com.xone.model.hibernate.entity.Person" table="T_PERSON">
		<id name="id" type="java.lang.Long">
			<column name="ID" length="20" not-null="true"/>
			<generator class="increment" />
		</id>
		<property name="username" type="java.lang.String">
			<column name="USERNAME" length="255"/>
		</property>
		<property name="password" type="java.lang.String">
			<column name="PASSWORD" length="255"/>
		</property>
		<property name="nickName" type="java.lang.String">
			<column name="NICK_NAME" length="255"/>
		</property>
		<property name="cellphone" type="java.lang.String">
			<column name="CELLPHONE" length="255"/>
		</property>
		<property name="contact" type="java.lang.String">
			<column name="CONTACT" length="255"/>
		</property>
		<property name="qq" type="java.lang.String">
			<column name="QQ" length="255"/>
		</property>
		<property name="email" type="java.lang.String">
			<column name="EMAIL" length="255"/>
		</property>
		<property name="addrLocation" type="java.lang.Long">
			<column name="ADDR_LOCATION" length="255"/>
		</property>
		<property name="credit" type="java.lang.String">
			<column name="CREDIT" length="2"/>
		</property>
		<property name="userLevel" type="java.lang.String">
			<column name="USER_LEVEL" length="2"/>
		</property>
		<property name="userApply" type="java.lang.Long">
			<column name="USER_APPLY" length="20"/>
		</property>
		<property name="dateApply" type="java.util.Date">
			<column name="DATE_APPLY" length="19"/>
		</property>
		<property name="userCheck" type="java.lang.Long">
			<column name="USER_CHECK" length="20"/>
		</property>
		<property name="dateCheck" type="java.util.Date">
			<column name="DATE_CHECK" length="19"/>
		</property>
		<property name="flagDeleted" type="java.lang.String">
			<column name="FLAG_DELETED" length="1"/>
		</property>
		<property name="userCreated" type="java.lang.Long">
			<column name="USER_CREATED" length="20"/>
		</property>
		<property name="dateCreated" type="java.util.Date">
			<column name="DATE_CREATED" length="19"/>
		</property>
		<property name="userUpdated" type="java.lang.Long">
			<column name="USER_UPDATED" length="20"/>
		</property>
		<property name="lastUpdated" type="java.util.Date">
			<column name="LAST_UPDATED" length="19"/>
		</property>
    </class>
</hibernate-mapping>
