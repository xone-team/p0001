<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xone.model.hibernate.entity.Address" table="T_ADDRESS">
		<id name="id" type="java.lang.Long">
			<column name="ID" length="20" not-null="true" />
			<generator class="increment" />
		</id>
		<property name="provinceCode" type="java.lang.String">
			<column name="PROVINCE_CODE" length="20" />
		</property>
		<property name="cityCode" type="java.lang.String">
			<column name="CITY_CODE" length="20" />
		</property>
		<property name="districtCode" type="java.lang.String">
			<column name="DISTRICT_CODE" length="20" />
		</property>
		<property name="detail" type="java.lang.String">
			<column name="DETAIL" length="1023" />
		</property>
		<property name="zip" type="java.lang.String">
			<column name="ZIP" length="10" />
		</property>
		<property name="flagDeleted" type="java.lang.String">
			<column name="FLAG_DELETED" length="1" />
		</property>
		<property name="userCreated" type="java.lang.Long">
			<column name="USER_CREATED" length="20" />
		</property>
		<property name="dateCreated" type="java.util.Date">
			<column name="DATE_CREATED" length="19" />
		</property>
		<property name="userUpdated" type="java.lang.Long">
			<column name="USER_UPDATED" length="20" />
		</property>
		<property name="lastUpdated" type="java.util.Date">
			<column name="LAST_UPDATED" length="19" />
		</property>
	</class>
</hibernate-mapping>