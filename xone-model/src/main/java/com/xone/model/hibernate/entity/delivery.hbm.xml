<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>  
    <class name="com.xone.model.hibernate.entity.Delivery" table="t_delivery">
		<id name="id" type="java.lang.Long">
			<column name="ID" length="20" not-null="true"/>
			<generator class="increment" />
		</id>
		<property name="productId" type="java.lang.Long">
			<column name="PRODUCT_ID" length="20"/>
		</property>
		<property name="marketarea" type="java.lang.String">
			<column name="MARKETAREA" length="255"/>
		</property>
		<property name="determini" type="java.lang.String">
			<column name="DETERMINI" length="255"/>
		</property>
		<property name="loadtime" type="java.lang.String">
			<column name="LOADTIME" length="255"/>
		</property>
		<property name="loadaddress" type="java.lang.String">
			<column name="LOADADDRESS" length="255"/>
		</property>
		<property name="boxNum" type="java.lang.String">
			<column name="BOX_NUM" length="20"/>
		</property>
		<property name="unitNum" type="java.lang.String">
			<column name="UNIT_NUM" length="20"/>
		</property>
		<property name="boxTotal" type="java.lang.String">
			<column name="BOX_TOTAL" length="20"/>
		</property>
		<property name="totalWeight" type="java.lang.String">
			<column name="TOTAL_WEIGHT" length="20"/>
		</property>
		<property name="flagPass" type="java.lang.String">
			<column name="FLAG_PASS" length="1"/>
		</property>
		<property name="remark" type="java.lang.String">
			<column name="REMARK" length="1023"/>
		</property>
		<property name="userApply" type="java.lang.Long">
			<column name="USER_APPLY" length="20"/>
		</property>
		<property name="dateApply" type="java.util.Date">
			<column name="DATE_APPLY" length="19"/>
		</property>
		<property name="userCheck" type="java.lang.Long">
			<column name="USER_CHECK" length="20"/>
		</property>
		<property name="dateCheck" type="java.util.Date">
			<column name="DATE_CHECK" length="19"/>
		</property>
		<property name="flagDeleted" type="java.lang.String">
			<column name="FLAG_DELETED" length="1"/>
		</property>
		<property name="userCreated" type="java.lang.Long">
			<column name="USER_CREATED" length="20"/>
		</property>
		<property name="dateCreated" type="java.util.Date">
			<column name="DATE_CREATED" length="19"/>
		</property>
		<property name="userUpdated" type="java.lang.Long">
			<column name="USER_UPDATED" length="20"/>
		</property>
		<property name="lastUpdated" type="java.util.Date">
			<column name="LAST_UPDATED" length="19"/>
		</property>
    </class>
</hibernate-mapping>
